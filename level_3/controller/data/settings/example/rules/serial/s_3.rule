# fss-000d

setting:
  name "Serial 3"
  need serial s_2

script:
  start {
    echo "Serial 3: sleeping $(date -u)"
    sleep 1
    echo "Serial 3: slept    $(date -u)"
  }

  stop {
    echo "Serial 3: stopping, sleeping $(date -u)"
    sleep 1
    echo "Serial 3: stopping, slept    $(date -u)"
  }
