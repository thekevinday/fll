# fss-000d

setting:
  name "Serial 4"
  need serial s_3

script:
  start {
    echo "Serial 4: sleeping $(date -u)"
    sleep 1
    echo "Serial 4: slept    $(date -u)"
  }

  stop {
    echo "Serial 4: stopping, sleeping $(date -u)"
    sleep 1
    echo "Serial 4: stopping, slept    $(date -u)"
  }
