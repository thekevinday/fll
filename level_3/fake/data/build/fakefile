# fss-0005 iki-0002

settings:
  load_build yes
  fail exit

  compiler gcc
  linker ar

  environment

  parameter verbose +v
  parameter verbose_fll +V

main:

  #build

  print beginning
  print return = 'parameter:"return"' (This needs to be fixed, always set return value after each line is operated).

  if defined parameter verbose verbose_fll
    print success defined parameter.
  else
    print failure defined parameter.

  if defined environment SHELL PS1
    print success defined environment.
  else
    print failure defined environment.
